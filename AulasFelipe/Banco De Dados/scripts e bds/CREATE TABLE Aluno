CREATE TABLE ALUNO (
    --PRIMARY KEY: é a chave primária da tabela!
    --AUTOINCREMENT:utilizado para somar +1 numero automaticamente
    --NOT NULL: é para não deixar  que o campo tenha valor vaziou ou nulo
    ra INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    rg TEXT NOT NULL,
    escolaridade TEXT NOT NULL,
    email TEXT NOT NULL,
    nome Text NOT NULL,
    endereço TEXT NOT NULL
    
);